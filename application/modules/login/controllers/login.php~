<?php if ( ! defined('BASEPATH')) exit('No direct script access allowed');

class Login extends CI_Controller {

	function __construct()
	 {
		parent::__construct();
		$this->load->helper(array('url','html'));
		$this->load->model('m_login');
	 }
	
	function index(){
	 $this->check_login();
	}
	
	
	
	
}
//end of file
